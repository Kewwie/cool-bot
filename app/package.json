{
  "name": "coolbot",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "watch": "tsc --watch",
    "watch:run": "tsc-watch --build --onSuccess \"npm run dev\"",
    "watch:dev": "nodemon --watch src --ext ts,js,json --exec npm run dev",
    "build": "tsc --build",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^4.6.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@discordjs/voice": "^0.17.0",
    "class-validator": "^0.14.1",
    "discord-api-types": "^0.37.79",
    "discord.js": "^14.12.1",
    "dotenv": "^16.4.5",
    "mongodb": "^5.9.2",
    "typeorm": "^0.3.20"
  }
}
